<% @title = 'Edit timecard entry' %>
<h1>Edit timecard entry</h1>
<p><%= error_messages_for 'timecard_entry' %></p>
<% form_tag timecard_entry_path(@timecard_entry), :method => 'put' do %>
  <%= render :partial => 'form' %>
  <%= submit_tag 'Update' %>
  <p>
    <%= link_to 'Back', timecard_entries_path %>
    <%= link_to 'Destroy', timecard_entry_path(@timecard_entry), :confirm => 'Are you sure?', :method => :delete %>
  </p>
<%- end %>
