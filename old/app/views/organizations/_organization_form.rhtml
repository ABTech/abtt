<% begin 
url = organization_path(@org)
rescue
url = new_organization_path
end %>
<% form_tag url, :method => :put do -%>
  Name:
  <input type="text"  name="organization[name]" value="<%= h @org.name %>" name=/>
  <br />
  Organization Email: 
  <%= text_field_tag "organiztion[org_email]" %><br />
  <%= submit_tag "Submit" %>
<%- end %>
